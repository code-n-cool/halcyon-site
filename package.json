{
  "name": "halcyon",
  "version": "0.1.0",
  "description": "A minimal, dark blue theme for VS Code, Sublime Text, Atom, and more.",
  "author": "Brittany Chiang <brittany.chiang@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/bchiang7/halcyon-site"
  },
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
